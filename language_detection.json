  "threat_intelligence_analysis_guide": {
    "_workflow": [
      "1. Extract all strings from FLOSS output",
      "2. Search for Windows API function names (GetSystemDefaultLangID, etc.)",
      "3. Search for LCID hex values (0x0419, 0x0422, etc.)",
      "4. Search for locale codes (en-US, ru-RU, etc.)",
      "5. Detect Unicode characters in different scripts",
      "6. Look for language-specific filenames (_en.txt, _ru.dll)",
      "7. Identify encoding declarations (CP1251, UTF-8)",
      "8. Cross-reference found languages with targeting categories"
    ],
    "_interpretation": {
      "broad_western_targeting": "EN + DE + FR + ES + IT = targeting Western Europe/US",
      "cis_exclusion": "API calls + CIS LCID checks + exit() = Russian malware avoiding prosecution",
      "single_language": "Only ONE language = highly targeted attack or regional malware",
      "missing_major_language": "Has EN/DE/ES but NO FR = France specifically excluded",
      "asia_focus": "ZH + JA + KO = Asia-Pacific targeting",
      "syria_exclusion": "ar-SY excluded alongside CIS = GandCrab/REvil signature"
    },
    "_confidence_levels": {
